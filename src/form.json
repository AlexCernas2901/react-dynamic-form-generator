{
  "title": "User Creation",
  "action": "http://localhost:3000/server.php",
  "method": "POST",
  "data": [{
  "section": {
      "title": "User Data",
      "class": "col-md-12",
      "fields": [{
          "field": {
          "id": "name_id",
          "class": "form-control",
          "label": "First Name",
          "type": "text",
          "data": "string",
          "pattern": "^[a-zA-ZÀ-ÖØ-öø-ÿ']+([ ][a-zA-ZÀ-ÖØ-öø-ÿ']+)*$",
          "placeholder": "Enter your first name",
          "required": true,
          "name": "first_name",
          "minLength": 6,
          "maxLength": 25
          }
      },
      {
          "field": {
          "id": "lastname_id",
          "class": "form-control",
          "label": "Last Name",
          "type": "text",
          "data": "string",
          "pattern": "^[a-zA-ZÀ-ÖØ-öø-ÿ']+([ ][a-zA-ZÀ-ÖØ-öø-ÿ']+)*$",
          "placeholder": "Enter your last name",
          "required": true,
          "name": "last_name",
          "minLength": 6,
          "maxLength": 25
          }
      },
      {
          "field": {
          "id": "email_id",
          "class": "form-control",
          "label": "Email",
          "type": "email",
          "data": "string",
          "pattern": "^[a-zA-Z0-9]+@[a-zA-Z0-9]+\\.[A-Za-z]+$",
          "placeholder": "Enter your email",
          "required": true,
          "name": "email",
          "minLength": 2,
          "maxLength": 25
          }
      },
      {
          "field": {
          "id": "password_id",
          "class": "form-control",
          "label": "Password",
          "type": "password",
          "data": "string",
          "pattern": "^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[\\W]).{6,20}$",
          "placeholder": "Enter your password",
          "required": true,
          "name": "password"
          }
      },
      {
          "field": {
          "id": "confirmpassword_id",
          "class": "form-control",
          "label": "Confirm Password",
          "type": "password",
          "data": "string",
          "pattern": "^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z])(?=.*[\\W]).{6,20}$",
          "placeholder": "Enter your password",
          "required": true,
          "name": "confirmPassword"
          }
      },
      {
          "field": {
          "id": "phonenumber_id",
          "class": "form-control",
          "label": "Phone Number",
          "type": "text",
          "data": "string",
          "pattern": "^\\+[0-9]{2} ?[0-9]{3} ?[0-9]{3} ?[0-9]{4}$",
          "placeholder": "Enter your phone number",
          "required": true,
          "name": "phone_number"
          }
      },
      {
          "field": {
          "id": "borndate_id",
          "class": "form-control",
          "label": "Date of Birth",
          "type": "date",
          "data": "date",
          "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
          "placeholder": "Enter your date of birth",
          "required": true,
          "name": "birth_date",
          "min": "2006-01-01"
          }
      }
      ]
  }
  }]
}
